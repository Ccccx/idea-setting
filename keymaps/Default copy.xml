<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="shift ctrl alt space" />
  </action>
  <action id="randomBackgroundImage">
    <mouse-shortcut keystroke="button4" />
  </action>
</keymap>